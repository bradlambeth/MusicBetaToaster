{
   "background_page": "background.html",
   "content_scripts": [ {
      "js": [ "song_notifier.js" ],
      "matches": [ "http://music.google.com/*", "https://music.google.com/*" ],
      "run_at": "document_end"
   } ],
   "icons": {
      "128": "logo-128x128.png",
      "16": "logo-16x16.png",
      "48": "logo-48x48.png"
   },
   "name": "Music Beta Toaster",
   "permissions": [ "tabs", "notifications", "http://music.google.com/*", "https://music.google.com/*" ],
   "version": "1.0.1"
}
